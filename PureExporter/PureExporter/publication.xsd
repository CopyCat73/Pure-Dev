                        </xs:annotation>
                    </xs:element>
                    <xs:element name="edition" type="commons:string_64" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                The edition of the publication
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="printIsbns" type="isbns" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                The printed isbns
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="electronicIsbns" type="isbns" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                The electronic isbns
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="series" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                The publication serie
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="publisher" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                The publisher of the publication
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="bookType">
        <xs:complexContent>
            <xs:extension base="publicationType">
                <xs:sequence>
